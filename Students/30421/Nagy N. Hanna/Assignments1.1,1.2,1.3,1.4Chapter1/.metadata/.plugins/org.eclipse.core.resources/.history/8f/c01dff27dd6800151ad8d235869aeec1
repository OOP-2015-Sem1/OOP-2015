import java.util.*;

public class MainSumOfMultiples {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner inputNumber= new Scanner(System.in);
		System.out.println("Give a number: ");
		int number=inputNumber.nextInt();
		System.out.print("The sum of multiplies 3,5 below the given number ");
		System.out.println(number+" is "+sumOfMultipliesOf3_5(number));
		

	}
	
	public static int sumOfMultipliesOf3_5(int n){
		int sum=0;
		for(int i=1;i<n;i++){
			if(i%3==0 || i%5==0) {
				sum += i;
			}
		}
		return sum;
	}

}
